    <GlobalPackageReference Include="Microsoft.CodeAnalysis.Analyzers" Version="2.9.8" />
    <GlobalPackageReference Include="Microsoft.Build.Artifacts" Version="2.0.20" />
    <GlobalPackageReference Include="Microsoft.NetFramework.Analyzers" Version="2.9.8" />
    <GlobalPackageReference Include="Roslyn.Diagnostics.Analyzers" Version="2.9.8" />
    <GlobalPackageReference Include="Roslynator.Analyzers" Version="2.3.0" />
    <GlobalPackageReference Include="Text.Analyzers" Version="2.6.4" />
  </ItemGroup>
